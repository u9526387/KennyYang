<resources>

<!-- Base application theme. -->

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />


    <style name="AppTheme.PopupOverlay" parent="Base.ThemeOverlay.AppCompat.Dark">
        <item name="android:actionMenuTextColor">@color/textColorPrimary</item>
    </style>

    <style name="App.Theme.Kenny" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/dot_dark_screen2</item>
        <item name="colorPrimaryDark">@color/KennyYangTitile</item>
        <item name="colorAccent">@color/colorPrimary_pair_check</item>
        <item name="android:colorBackground">@color/button_true</item>
        <item name="android:colorForeground">@color/black</item>
        <item name="android:navigationBarColor">@color/black</item>
        <item name="android:statusBarColor">@color/darkgreen</item>
        <item name="android:textColor">@color/textColorPrimary</item>
        <item name="android:actionMenuTextColor">@color/textColorPrimary</item>
        <item name="android:textSize">20sp</item>


    </style>

    <style name="Theme.App.Kenny.Light.DarkActionBar" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:colorPrimary">@color/bg_screen2</item>
        <item name="android:colorPrimaryDark">@color/holo_light_primary_dark</item>
        <item name="android:colorBackground">@color/button_true</item>
    </style>

</resources>