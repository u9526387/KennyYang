<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FillRentInfo2">




    <TextView
        android:id="@+id/gender3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="72dp"
        android:paddingTop="5dp"
        android:text="出租地址:"
        android:textColor="#000000"
        android:textSize="28dp"
        android:layout_alignParentLeft="true" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="124dp"
        android:entries="@array/city"
        android:layout_alignParentLeft="true" />

    <TextView
        android:id="@+id/gender2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/editText"
        android:layout_marginEnd="113dp"
        android:paddingTop="5dp"
        android:text="號"
        android:textColor="#000000"
        android:textSize="28dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="113dp" />

    <TextView
        android:id="@+id/gender5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/gender2"
        android:layout_marginStart="-44dp"
        android:layout_toEndOf="@+id/editText8"
        android:paddingTop="5dp"
        android:text="樓"
        android:textColor="#000000"
        android:textSize="28dp"
        android:layout_marginLeft="-44dp"
        android:layout_toRightOf="@+id/editText8" />

    <TextView
        android:id="@+id/gender6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/gender2"
        android:layout_toEndOf="@+id/editText7"
        android:paddingTop="5dp"
        android:text="弄"
        android:textColor="#000000"
        android:textSize="28dp"
        android:layout_toRightOf="@+id/editText7" />

    <TextView
        android:id="@+id/gender4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/editText"
        android:layout_toEndOf="@+id/editText"
        android:paddingTop="5dp"
        android:text="巷"
        android:textColor="#000000"
        android:textSize="28dp"
        android:layout_toRightOf="@+id/editText" />

    <TextView
        android:id="@+id/FillRentInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFCDD2"
        android:gravity="center"
        android:text="房屋資料"
        android:textColor="#000000"
        android:textSize="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="182dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:layout_alignParentLeft="true" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/gender2"
        android:layout_marginStart="97dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="97dp" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/gender2"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:layout_alignParentRight="true"
        android:layout_marginRight="44dp" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/gender2"
        android:layout_marginEnd="139dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:layout_alignParentRight="true"
        android:layout_marginRight="139dp" />




</RelativeLayout>