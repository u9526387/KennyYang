<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.u9526.myapplication.homepage">

    <TextView
        android:id="@+id/firstName5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/firstName6"
        android:layout_below="@+id/Food"
        android:layout_marginTop="-198dp"
        android:text="水電修繕"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignLeft="@+id/firstName6" />

    <TextView
        android:id="@+id/firstName12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageButton11"
        android:layout_marginStart="43dp"
        android:layout_marginTop="-53dp"
        android:text="布告欄"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="43dp" />

    <TextView
        android:id="@+id/firstName14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/firstName5"
        android:layout_below="@+id/imageButton11"
        android:layout_marginTop="-53dp"
        android:text="使用說明"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignLeft="@+id/firstName5" />

    <TextView
        android:id="@+id/firstName13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Food"
        android:layout_marginStart="29dp"
        android:layout_marginTop="-198dp"
        android:text="食物外送"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="29dp" />

    <TextView
        android:id="@+id/firstName11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/RentAndFind"
        android:layout_marginStart="29dp"
        android:text="青銀共居"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="29dp" />

    <TextView
        android:id="@+id/firstName6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/RentAndFind"
        android:layout_marginEnd="33dp"
        android:text="健康照護"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="33dp" />

    <TextView
        android:id="@+id/ChooseIdentity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFCDD2"
        android:gravity="center"
        android:text="吾居無束"
        android:textColor="#000000"
        android:textSize="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/RentAndFind"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="113dp"
        android:adjustViewBounds="false"
        android:background="@drawable/button_border"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/family"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="36dp" />

    <ImageButton
        android:id="@+id/HealthCare"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/RentAndFind"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:background="@drawable/button_border"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/care" />

    <ImageButton
        android:id="@+id/Food"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignLeft="@+id/RentAndFind"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/RentAndFind"
        android:layout_marginBottom="198dp"
        android:background="@drawable/button_border"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/food" />

    <ImageButton
        android:id="@+id/Repair"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignLeft="@+id/HealthCare"
        android:layout_alignStart="@+id/HealthCare"
        android:layout_alignTop="@+id/Food"
        android:background="@drawable/button_border"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/tools" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/RentAndFind"
        android:layout_marginBottom="53dp"
        android:scaleType="fitXY"
        android:background="@drawable/button_border"
        app:srcCompat="@mipmap/write"
        android:layout_alignLeft="@+id/RentAndFind" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignStart="@+id/HealthCare"
        android:layout_below="@+id/firstName5"
        android:scaleType="fitXY"
        android:background="@drawable/button_border"
        app:srcCompat="@mipmap/question"
        android:layout_alignLeft="@+id/HealthCare" />


</RelativeLayout>
