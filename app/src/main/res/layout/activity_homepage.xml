<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.u9526.myapplication.homepage">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/title" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="220dp"
        android:layout_height="110dp"
        android:layout_below="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:backgroundTint="@android:color/background_light"
        android:onClick="jumpToteenAndold"
        app:srcCompat="@drawable/app1" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_below="@+id/imageButton3"
        android:layout_toLeftOf="@+id/imageButton10"
        android:layout_toStartOf="@+id/imageButton10"
        android:backgroundTint="@android:color/background_light"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/app2" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_below="@+id/imageButton3"
        android:layout_centerHorizontal="true"
        android:backgroundTint="@android:color/background_light"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/app3" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_below="@+id/imageButton3"
        android:layout_toEndOf="@+id/imageButton10"
        android:layout_toRightOf="@+id/imageButton10"
        android:backgroundTint="@android:color/background_light"
        app:srcCompat="@drawable/app4" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="wrap_content"
        android:layout_height="120dp"
        android:layout_alignLeft="@+id/imageButton9"
        android:layout_alignStart="@+id/imageButton9"
        android:layout_below="@+id/imageButton9"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:backgroundTint="@android:color/background_light"
        app:srcCompat="@drawable/app5" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="wrap_content"
        android:layout_height="120dp"
        android:layout_below="@+id/imageButton10"
        android:layout_toEndOf="@+id/imageButton12"
        android:layout_toRightOf="@+id/imageButton12"
        android:backgroundTint="@android:color/background_light"
        app:srcCompat="@drawable/app6" />


</RelativeLayout>
