<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.u9526.myapplication.homepage">

    <TextView
        android:id="@+id/firstName5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Repair"
        android:layout_alignParentRight="true"
        android:layout_marginRight="45dp"
        android:text="水電修繕"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignLeft="@+id/firstName6" />

    <TextView
        android:id="@+id/firstName12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/imageButton11"
        android:layout_below="@+id/imageButton11"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="55dp"
        android:text="布告欄"
        android:textColor="#000000"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/firstName14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton13"
        android:layout_alignParentRight="true"
        android:layout_marginRight="45dp"
        android:text="使用說明"
        android:textColor="#000000"
        android:textSize="30dp"
  />

    <TextView
        android:id="@+id/firstName13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Food"
        android:text="食物外送"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="45dp" />

    <TextView
        android:id="@+id/firstName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/RentAndFind"
        android:layout_marginLeft="45dp"
        android:text="青銀共居"
        android:textColor="#000000"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/firstName6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/HealthCare"
        android:layout_alignParentRight="true"
        android:layout_marginRight="45dp"
        android:text="健康照護"
        android:textColor="#000000"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/ChooseIdentity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFCDD2"
        android:gravity="center"
        android:text="吾居無束"
        android:textColor="#000000"
        android:textSize="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/RentAndFind"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/button_border"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/family"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="5dp"
        android:onClick="jumpTorent_homepage"/>

    <ImageButton
        android:id="@+id/HealthCare"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/RentAndFind"
        android:layout_marginRight="50dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/button_border"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/care" />

    <ImageButton
        android:id="@+id/Food"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="250dp"
        android:background="@drawable/button_border"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/food" />

    <ImageButton
        android:id="@+id/Repair"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignLeft="@+id/HealthCare"
        android:layout_alignStart="@+id/HealthCare"
        android:layout_alignTop="@+id/Food"
        android:background="@drawable/button_border"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/tools" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_alignLeft="@+id/RentAndFind"
        android:layout_alignStart="@+id/RentAndFind"
        android:layout_alignParentTop="true"
        android:layout_marginTop="400dp"
        android:scaleType="fitXY"
        android:background="@drawable/button_border"
        app:srcCompat="@mipmap/write"
         />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:scaleType="fitXY"
        android:background="@drawable/button_border"
        app:srcCompat="@mipmap/question"
        android:layout_alignLeft="@+id/HealthCare"
        android:layout_marginTop="400dp"
 />

    <Button
        android:id="@+id/buttonLogout"
        android:layout_width="77dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:text="登出"
        android:layout_alignParentRight="true" />


</RelativeLayout>
